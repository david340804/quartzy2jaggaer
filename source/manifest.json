{
    "name": "Quartzy2Jaggaer",
    "version": "1.0",
    "description": "Stop copy pasting from Quartzy",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {},
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
    {
        "matches": ["https://solutions.sciquest.com/apps/Router/NonCatalogForm?*"],
        "js": ["nonCatalogForm.js"]
    }
    ]
}